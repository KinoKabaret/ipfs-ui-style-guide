include(../features.fea);

@TNUM_L = [
  one two seven
  one.1 four.1 six.1 nine.1
  period comma
];
@TNUM_R = [
  one.tnum two.tnum seven.tnum
  one.tnum.1 four.tnum.1 six.tnum.1 nine.tnum.1
  period.tnum comma.tnum
];

feature tnum {
  sub @TNUM_L by @TNUM_R;
} tnum;

feature zero {
  sub zero by zero.slash;
} zero;
